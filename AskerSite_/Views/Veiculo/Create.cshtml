@model Estacionamento.Models.VeiculoModel

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>VeiculoModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Placa" class="control-label"></label>
                <input asp-for="Placa" class="form-control" />
                <span asp-validation-for="Placa" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Entrada" class="control-label"></label>
                <input asp-for="Entrada" class="form-control" value="@DateTime.Now" />
                <span asp-validation-for="Entrada" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TempoCobrado" class="control-label">Tempo Cobrado</label>
                <input asp-for="TempoCobrado" class="form-control" />
                <span asp-validation-for="TempoCobrado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PrecoEstatacionamento" class="control-label">Preco do estatacionamento</label>
                <input asp-for="PrecoEstatacionamento" class="form-control" />
                <span asp-validation-for="PrecoEstatacionamento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
